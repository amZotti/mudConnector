<%= link_to "Log out", :session, method: :delete %>
<h1>Map</h1>
<pre id="ascii-map"></pre>

<%= form_for :movements, class: "movements" do |f| %>
  <table border="0">
  <tr>
    <td>
      <%= f.submit "west", class: "direction" %>
    </td>

    <td>
      <%= f.submit "north", class: "direction" %>
    </td>

    <td>
      <%= f.submit "south", class: "direction" %>
    </td>

    <td>
      <%= f.submit "east", class: "direction" %>
    </td>
  </tr>
</table>
<% end %>

<p id="user-stats"></p>

<div class="character-display"></div>

<ul id="display">
</ul>
